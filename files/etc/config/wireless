config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/jzmmc_v1.2.1/mmc_host/mmc1/mmc1:0001/mmc1:0001:2'
	option htmode	
	option disabled 1

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     airdisk-jz-x1000
	option encryption none

config wifi-iface
	option device   radio0
	option network  wan
	option mode     sta
	option ssid     AirDiskGroup
	option encryption mixed-psk+tkip+ccmp
	option key      airdisk2014
